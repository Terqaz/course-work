<p>{{ channel.description }}</p>
<h5>Ветки канала:</h5>
<ol class="list-group">
    {% for branch in branches %}
        <li class="channel-branches list-group-item d-flex justify-content-between align-items-start ms-2"
            data-provider-id="{{ branch.id }}"
        >
            <div>
                <div>{{ branch.name }}</div>
            </div>
        </li>
    {% endfor %}
</ol>
